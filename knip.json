{
  "$schema": "./node_modules/knip/schema.json",
  "ignore": [
    "**/.conductor/**",
    "**/.git/**",
    "**/.*",
    ".git",
    "**/.git/hooks/**",
    "**/scripts/postinstall.mjs",
    "**/scripts/preinstall.mjs",
    "**/*.generated.*",
    "**/tests/setup.ts",
    "**/tests/types.d.ts",
    "**/tests/create-fixtures.ts",
    "bun-test-setup.ts",
    "**/apps/cli/src/components/Terminal/**",
    "**/apps/cli/tests/streaming/**"
  ],
  "ignoreBinaries": ["microfrontends"],
  "ignoreDependencies": ["tailwindcss"],
  "ignoreUnresolved": ["@types/bun", "@effect/language-service"],
  "ignoreExportsUsedInFile": {
    "interface": true
  }
}
