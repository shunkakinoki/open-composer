{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_BRANCH_URL",
    "VERCEL_REGION",
    "VERCEL_DEPLOYMENT_ID"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**"
      ]
    },
    "build:generate": {
      "inputs": [
        "package.json"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "tests/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "prepublishOnly": {
      "dependsOn": [
        "^build",
        "^prepublishOnly"
      ]
    },
    "__all__": {
      "dependsOn": [
        "build",
        "type-check",
        "test",
        "//#__format__",
        "//#__lint__"
      ]
    },
    "//#__lint__": {
      "dependsOn": [
        "//#__check__",
        "//#__fix__"
      ]
    },
    "//#__format__": {
      "dependsOn": [
        "//#__fix__"
      ]
    },
    "//#__check__": {
      "dependsOn": [
        "^type-check",
        "//#biome:check:check",
        "//#biome:format:check",
        "//#npm-package-json:check",
        "//#sort-package-json:check",
        "//#knip:check"
      ]
    },
    "//#__fix__": {
      "dependsOn": [
        "//#biome:check:fix",
        "//#biome:format:fix",
        "//#npm-package-json:fix",
        "//#sort-package-json:fix",
        "//#knip:fix"
      ]
    },
    "//#__upgrade__": {
      "dependsOn": [
        "//#bun:upgrade",
        "//#ncu:upgrade"
      ]
    },
    "//#biome:check:check": {
      "cache": false
    },
    "//#biome:check:fix": {
      "cache": false
    },
    "//#biome:format:check": {
      "cache": false,
      "dependsOn": [
        "//#biome:check:check",
        "//#npm-package-json:check",
        "//#sort-package-json:check"
      ]
    },
    "//#biome:format:fix": {
      "cache": false,
      "dependsOn": [
        "//#biome:check:fix",
        "//#npm-package-json:fix",
        "//#sort-package-json:fix"
      ]
    },
    "//#bun:upgrade": {
      "cache": false
    },
    "//#knip:check": {
      "cache": false
    },
    "//#knip:fix": {
      "cache": false
    },
    "//#ncu:upgrade": {
      "cache": false
    },
    "//#npm-package-json:check": {
      "cache": false
    },
    "//#npm-package-json:fix": {
      "cache": false
    },
    "//#sort-package-json:check": {
      "cache": false
    },
    "//#sort-package-json:fix": {
      "cache": false
    },
  }
}